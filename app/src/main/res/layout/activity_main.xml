<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.user.myapplication.MainActivity">

    <ImageButton
        android:id="@+id/imageButtonPhone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="showDial"
        app:srcCompat="@drawable/ic_phone_black_24dp"
        tools:layout_editor_absoluteX="147dp"
        tools:layout_editor_absoluteY="426dp" />

    <ImageButton
        android:id="@+id/imageButtonMail"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/ic_email_black_24dp"
        tools:layout_editor_absoluteX="218dp"
        tools:layout_editor_absoluteY="426dp" />

    <ImageButton
        android:id="@+id/imageButtonWeb"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="visitBAIT20713"
        app:srcCompat="@drawable/ic_web_black_24px"
        tools:layout_editor_absoluteX="77dp"
        tools:layout_editor_absoluteY="426dp" />

    <ImageButton
        android:id="@+id/imageButtonHome"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="showMain"
        app:srcCompat="@drawable/ic_home_black_24dp"
        tools:layout_editor_absoluteX="292dp"
        tools:layout_editor_absoluteY="426dp" />

    <EditText
        android:id="@+id/editTextName"
        android:layout_width="361dp"
        android:layout_height="42dp"
        android:ems="10"
        android:hint="@string/name"
        android:inputType="textPersonName"
        android:text="@string/name"
        tools:layout_editor_absoluteX="28dp"
        tools:layout_editor_absoluteY="305dp" />

    <EditText
        android:id="@+id/editTextEmail"
        android:layout_width="365dp"
        android:layout_height="48dp"
        android:ems="10"
        android:hint="@string/email"
        android:inputType="textPersonName"
        android:text="@string/email"
        tools:layout_editor_absoluteX="30dp"
        tools:layout_editor_absoluteY="355dp" />

    <Button
        android:id="@+id/button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="updateProfile"
        android:text="@string/update"
        tools:layout_editor_absoluteX="155dp"
        tools:layout_editor_absoluteY="510dp" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:contentDescription="@string/profile_picture"
        app:srcCompat="@drawable/user"
        tools:layout_editor_absoluteX="77dp"
        tools:layout_editor_absoluteY="16dp" />

</android.support.constraint.ConstraintLayout>
